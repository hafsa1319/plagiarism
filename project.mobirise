          "_customHTML": "<section data-bs-version=\"5.1\" class=\"pricing1\" group=\"Pricing\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\" plugins=\"mbr-tabs\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"7\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Colored Text\" name=\"showColoredText\" condition=\"showTitle\" checked>\n        <input type=\"color\" title=\"Colored Text Color\" name=\"coloredText\" value=\"#DDFF55\" condition=\"showTitle && showColoredText\">\n        <header>Tabs</header>\n        <select title=\"Count\" name=\"bootstrapTabs\">\n            <option value=\"1\">1</option>\n            <option value=\"2\" selected>2</option>\n        </select>\n        <input type=\"color\" name=\"backgroundTabs\" title=\"Background Color\" value=\"#f3f4ef\">\n        <input type=\"color\" name=\"tabsColor\" title=\"Color\" value=\"#010101\">\n        <input type=\"color\" name=\"activeColor\" title=\"Active Color\" value=\"#166308\">\n        <header>Cards</header>\n        <select title=\"Count\" name=\"countCard\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\" selected>4</option>\n        </select>\n        <input type=\"color\" title=\"Card BG\" name=\"cardBg\" value=\"#f3f4ef\">\n        <input type=\"checkbox\" title=\"Popular Card\" name=\"showBestValue\" checked>\n        <input type=\"color\" title=\"Popular Card BG\" name=\"cardPopularBg\" value=\"#166308\" condition=\"showBestValue\">\n        <input type=\"checkbox\" title=\"Popular Card Icon\" name=\"showBestIcon\" checked condition=\"showBestValue\">\n        <input type=\"color\" title=\"Popular Icon Color\" name=\"bestIconColor\" value=\"#1B1F0A\" condition=\"showBestValue && showBestIcon\">\n        <input type=\"checkbox\" title=\"Card Label\" name=\"showLabel\" checked>\n        <input type=\"checkbox\" title=\"Card Price\" name=\"showCardPrice\" checked>\n        <input type=\"checkbox\" title=\"Card List\" name=\"showCardList\" checked>\n        <select title=\"List 1 Amount\" name=\"lists1Amount\" condition=\"showCardList\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\" selected>5</option>\n            <option value=\"6\">6</option>\n            <option value=\"7\">7</option>\n            <option value=\"8\">8</option>\n            <option value=\"9\">9</option>\n            <option value=\"10\">10</option>\n        </select>\n        <select title=\"List 2 Amount\" name=\"lists2Amount\" condition=\"showCardList\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\">6</option>\n            <option value=\"7\" selected>7</option>\n            <option value=\"8\">8</option>\n            <option value=\"9\">9</option>\n            <option value=\"10\">10</option>\n        </select>\n        <select title=\"List 3 Amount\" name=\"lists3Amount\" condition=\"showCardList\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option 